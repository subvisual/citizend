{
  "name": "@discovery-dao/web-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "bundle": "rm -rf ${OUT_DIR:-dist} .next && yarn build && next export -o ${OUT_DIR:-dist}",
    "dev": "next dev",
    "lint": "yarn lint:next && yarn lint:tsc && yarn prettier",
    "lint:next": "next lint",
    "lint:tsc": "tsc --noEmit",
    "precommit": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "start": "next start"
  },
  "lint-staged": {
    "!(*node_modules|.next)/**/*.@(ts|tsx)": [
      "bash -c tsc --noEmit"
    ],
    "!(*node_modules|.next)/**/*.@(ts|tsx|js)": [
      "prettier --write --ignore-unknown",
      "eslint"
    ],
    "package.json": [
      "sort-package-json"
    ],
    "yarn.lock": [
      "yarn-deduplicate"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@idos-network/idos-sdk": "0.2.10",
    "@kwilteam/kwil-js": "0.5.8",
    "@rainbow-me/rainbowkit": "^2.0.1",
    "@rainbow-me/rainbowkit-siwe-next-auth": "^0.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/query-async-storage-persister": "^5.28.13",
    "@tanstack/query-sync-storage-persister": "^5.28.13",
    "@tanstack/react-query": "^5.24.1",
    "@tanstack/react-query-persist-client": "^5.28.14",
    "@tanstack/react-table": "^8.16.0",
    "@types/node": "20.12.7",
    "autoprefixer": "10.4.19",
    "bcrypt": "^5.1.1",
    "clsx": "^2.0.0",
    "ethers": "^6.12.0",
    "near-api-js": "^3.0.3",
    "next": "^14.2.2",
    "postcss": "8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "siwe": "^2.3.2",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "viem": "2.9.21",
    "wagmi": "^2.5.20",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/lodash.merge": "^4.6.9",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "@vercel/style-guide": "^6.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.52.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "9.1.0",
    "lint-staged": "^15.2.2",
    "lodash.merge": "^4.6.2",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "0.5.14",
    "sort-package-json": "^2.8.0",
    "yarn-deduplicate": "^6.0.2"
  },
  "engines": {
    "node": ">=20.11.1",
    "npm": "please-use-yarn",
    "yarn": ">=1.22.0"
  }
}
